#ifndef GPIO_DRIVER_H_
#define GPIO_DRIVER_H_



//Clock Enable or Disable API

void GPIO_ClockControl();

//GPIO Init and GPIO Deint

void GPIO_Init(void);
void GPIO_DeInit(void);

//Read Write

void GPIO_ReadFromIn_Pin(void);
